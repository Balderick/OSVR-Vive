{
	"deviceVendor": "HTC",
	"deviceName": "Vive PRE",
	"author": "Georgiy Frolov <gfrolov@sensics.com>",
	"version": 1,
	"lastModified": "",
	"interfaces": {
		"tracker": {
			"count": 3,
			"bounded": true,
			"position": true,
			"orientation": true
		},
		"analog": {
			"count": 1
		},
		"button": {
			"count": 1
		}
	},
	"semantic": {
		"head": {
			"$target": "tracker/0",
			"proximity": "button/0"
		},
		"ipd": "analog/0",
		"controller": {
			"left": {
				"$target": "tracker/1"
			},
			"right": {
				"$target": "tracker/2"
			}
		}
	},
	"automaticAliases": {
		"/me/head": "semantic/head",
		"/me/ipd": "semantic/ipd",
		"/controller": "semantic/controller/*",
		"/me/hands/left": "semantic/controller/left",
		"/me/hands/right": "semantic/controller/right"
	}
}
